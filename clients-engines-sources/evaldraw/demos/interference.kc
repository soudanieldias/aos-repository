(x,y,t,&r,&g,&b)
yy = y*y;
d0 = sqrt((x-1)^2+yy);
d1 = sqrt((x+1)^2+yy);
d = t*.2; //rnd;
d0 = (d0-d)*16;
d1 = (d1-d)*16;
//d0 = cos(d0);
//d1 = cos(d1);
//d0 = abs(d0%4-2)-1;
//d1 = abs(d1%4-2)-1;
d0 = abs(fadd(d0,3*2^53)-3*2^53-d0)-1;
d1 = abs(fadd(d1,3*2^53)-3*2^53-d1)-1;
g = (d0+d1)*64+128;
r = d0*64+128;
b = d1*64+128;